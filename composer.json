{
	"name": "david63/resetpostcount",
	"type": "phpbb-extension",
	"description": "Adds an ACP option to allow the resetting of a user's post count.",
	"version": "3.1.0-RC1",
	"time": "2020-01-20",
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "David Wood",
			"role": "Developer"
		}],
	"require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "[**] Reset post count",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0"
    	},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "reset_post_count_version_file.json",
			"ssl": false
    	}
	}
}
